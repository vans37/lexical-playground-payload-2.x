{
  "name": "payload-example-custom-server",
  "description": "Payload custom server example.",
  "version": "1.0.0",
  "main": "dist/server.js",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts nodemon",
    "seed": "rm -rf media && cross-env PAYLOAD_PUBLIC_SEED=true PAYLOAD_DROP_DATABASE=true PAYLOAD_CONFIG_PATH=src/payload.config.ts ts-node src/server.ts",
    "build:payload": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:next": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NEXT_BUILD=true node dist/server.js",
    "build": "cross-env NODE_ENV=production yarn build:payload && yarn build:server && yarn copyfiles && yarn build:next",
    "serve": "cross-env PAYLOAD_CONFIG_PATH=dist/payload.config.js NODE_ENV=production node dist/server.js",
    "clean": "rimraf dist && rimraf .next && rimraf ./build",
    "eject": "yarn remove next react react-dom @next/eslint-plugin-next && ts-node eject.ts",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,pdf}\" dist/",
    "generate:types": "cross-env PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:types",
    "generate:graphQLSchema": "PAYLOAD_CONFIG_PATH=src/payload.config.ts payload generate:graphQLSchema",
    "lint": "eslint src",
    "lint:fix": "eslint --fix --ext .ts,.tsx src"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@innovixx/payload-icon-picker-field": "^0.0.4",
    "@lexical/file": "0.13.1",
    "@lexical/hashtag": "0.13.1",
    "@lexical/html": "0.13.1",
    "@lexical/react": "0.13.1",
    "@lexical/table": "0.13.1",
    "@lexical/utils": "0.13.1",
    "@nouance/payload-better-fields-plugin": "^1.4.1",
    "@payloadcms/bundler-webpack": "^1.0.0",
    "@payloadcms/db-mongodb": "^1.5.1",
    "@payloadcms/db-postgres": "^0.8.0",
    "@payloadcms/plugin-seo": "^2.3.0",
    "@payloadcms/richtext-lexical": "^0.11.1",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/react-input-mask": "^3.0.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cross-env": "^7.0.3",
    "dlv": "^1.1.3",
    "dot-prop": "^8.0.2",
    "dotenv": "^8.2.0",
    "embla-carousel-auto-scroll": "^8.0.2",
    "embla-carousel-autoplay": "^8.0.0",
    "embla-carousel-react": "^8.0.0",
    "escape-html": "^1.0.3",
    "eslint-config-next": "^14.0.2",
    "expect-type": "^0.19.0",
    "express": "^4.19.2",
    "formdata-node": "^6.0.3",
    "install": "^0.13.0",
    "jsdom": "^24.0.0",
    "lucide-react": "^0.356.0",
    "next": "14.2.5",
    "nextjs-better-unstable-cache": "^1.1.0",
    "payload": "2.25.0",
    "qs": "^6.12.1",
    "react": "^18.2.0",
    "react-dom": "^18",
    "react-hook-form": "^7.51.0",
    "react-icons": "^5.1.0",
    "react-input-mask": "^2.0.4",
    "react-photo-album": "^2.4.1",
    "rimraf": "^5.0.7",
    "server-only": "^0.0.1",
    "slugify": "^1.6.6",
    "tailwind-merge": "^2.2.1",
    "tailwindcss-animate": "^1.0.7",
    "type-fest": "^4.15.0",
    "yet-another-react-lightbox": "^3.21.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^13.4.8",
    "@payloadcms/eslint-config": "1.1.1",
    "@tailwindcss/typography": "^0.5.12",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^4.17.9",
    "@types/node": "^20",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "6.6.0",
    "@typescript-eslint/parser": "6.6.0",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "ts-node": "10.9.1",
    "typescript": "^5"
  }
}
